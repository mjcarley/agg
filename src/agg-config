#!/bin/sh
# src/agg-config.  Generated from agg-config.in by configure.

agg_cflags="-I/home/michael/Codes/include -Wall -Werror-implicit-function-declaration -Wstrict-prototypes -Wmissing-prototypes -Wmissing-declarations -D_GNU_SOURCE -fopenmp  -mavx -mavx2 -mfma -I/usr/include/glib-2.0 -I/usr/lib64/glib-2.0/include -I/home/michael/Codes/include"
agg_cppflags=""
openmp_cflags="-fopenmp"
##glib_libs="@glib_libs@"

prefix=/home/michael/Codes
exec_prefix=${prefix}
includedir=${prefix}/include

single_precision=no

usage()
{
	cat <<EOF
Usage: agg-config [OPTIONS] [LIBRARIES]
Options:
	[--prefix[=DIR]]
	[--exec-prefix[=DIR]]
	[--version]
	[--libs]
	[--cflags]
Libraries:
	agg
EOF
	exit $1
}

uniquify()
{
    echo $1 | awk '{
	for (i = 1; i <= NF; i++) {
	    unique = 1;
	    for (j = i + 1; j <= NF && unique; j++)
		if ($i == $j)
		    unique = 0;
            if (unique)
		printf ("%s ", $i);
	}
    }'
}

if test $# -eq 0; then
	usage 1 1>&2
fi

while test $# -gt 0; do
  case "$1" in
  -*=*) optarg=`echo "$1" | sed 's/[-_a-zA-Z0-9]*=//'` ;;
  *) optarg= ;;
  esac

  case $1 in
    --prefix=*)
      prefix=$optarg
      if test $exec_prefix_set = no ; then
        exec_prefix=$optarg
      fi
      ;;
    --prefix)
      echo_prefix=yes
      ;;
    --exec-prefix=*)
      exec_prefix=$optarg
      exec_prefix_set=yes
      ;;
    --exec-prefix)
      echo_exec_prefix=yes
      ;;
    --version)
      echo 0.1.0
      exit 0
      ;;
    --cflags)
      echo_cflags=yes
      ;;
    --libs)
      echo_libs=yes
      ;;
    agg)
      lib_agg=yes
      ;;
    *)
      usage 1 1>&2
      ;;
  esac
  shift
done

if test "$echo_prefix" = "yes"; then
	echo $prefix
fi
if test "$echo_exec_prefix" = "yes"; then
	echo $exec_prefix
fi
if test "$echo_cflags" = "yes"; then
    ##glib_cflags="$glib_cflags -I${prefix}/include"
    ##glib_cflags=`uniquify "$glib_cflags"`

    agg_cflags="-I${prefix}/include"
    agg_cflags=`uniquify "$agg_cflags"`
    echo $agg_cppflags $agg_cflags
fi
if test "$echo_libs" = "yes"; then
    ##glib_libs="-L${exec_prefix}/lib $lib_wmpi -L/home/michael/Codes/lib  $glib_libs -lm"
    ##glib_libs=`uniquify "$glib_libs"`

    #agg_libs="-L${exec_prefix}/lib -lagg -L/home/michael/Codes/lib -lm $agg_libs"
    agg_libs="-L${exec_prefix}/lib -lagg -ltriangle-api -lmatheval -lm  -ltriangle-api -lglib-2.0 -lblas -L/home/michael/Codes/lib -L/usr/lib64/gcc/x86_64-slackware-linux/11.2.0 -L/usr/lib64/gcc/x86_64-slackware-linux/11.2.0/../../../../lib64 -L/lib/../lib64 -L/usr/lib/../lib64 -L/usr/lib64/gcc/x86_64-slackware-linux/11.2.0/../../.. -lgfortran -lquadmath -lm -lglib-2.0 -lstdc++"
    agg_libs=`uniquify "$agg_libs"`
    echo ${agg_libs}
fi
